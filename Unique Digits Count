public class Example {
 
   public static void main(String[] args) {
 
      int number = 1212312340;
 
      int arr[] = new int[10];
 
      while (number > 0){
         int x = number % 10;
         arr[x] = arr[x] + 1;
         number = number / 10;
      } 
 
      for (int i = 0; i < arr.length; i++) {
         System.out.println("" + i + ": " + arr[i]);
      }
   }
}
 
